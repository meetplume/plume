{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "ext-zip": "*",
        "andreiio/blade-iconoir": "^4.7",
        "awcodes/palette": "^2.0",
        "bezhansalleh/filament-language-switch": "^4.0",
        "charlieetienne/filament-font-picker": "^1.0",
        "charlieetienne/palette-generator": "^1.0",
        "eneadm/ladder": "^1.3",
        "filament/filament": "^4.0",
        "intervention/image-laravel": "^1.5",
        "lara-zeus/spatie-translatable": "^1.0",
        "laravel/framework": "^12.0",
        "laravel/tinker": "^2.10.1",
        "league/html-to-markdown": "^5.1",
        "mcamara/laravel-localization": "^2.3",
        "panphp/pan": "^0.1.8",
        "phiki/phiki": "^2.0",
        "prezet/prezet": "^1.0",
        "rawilk/laravel-settings": "^3.4",
        "schmeits/filament-4-phosphor-icons": "^1.0",
        "secondnetwork/blade-tabler-icons": "^3.35"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.16",
        "fakerphp/faker": "^1.23",
        "laravel/boost": "^1.0",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-browser": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "pestphp/pest-plugin-livewire": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test"
        ],
        "setup": [
            "Composer\\Config::disableProcessTimeout",
            "composer install",
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "touch database/database.sqlite",
            "@php artisan key:generate --ansi",
            "php artisan migrate:fresh",
            "php artisan storage:link",
            "php artisan db:seed --class=AdminUserSeeder",
            "npm install",
            "npm run build",
            "@php -r \"shell_exec('which herd') ? shell_exec('herd secure') : print('Herd not found, skipping herd secure\\n');\"",
            "@php -r \"shell_exec('which herd') ? shell_exec('herd open') : print('Herd not found, skipping herd open\\n');\""
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "beta",
    "prefer-stable": true
}
